      "WebFetch",
      "WebSearch",
      "Bash(ls *)",
      "Bash(git commit *)",
      "Bash(git fetch *)",
      "Bash(git checkout *)",
      "Bash(git show *)",
      "Bash(git log *)",
      "Bash(git diff *)",
      "Bash(git branch *)",
      "Bash(git stash *)",
      "Bash(git status *)",
      "Bash(git rebase *)",
      "Bash(gh pr *)",
      "Bash(gh api *)",
      "Bash(gh issue *)"
